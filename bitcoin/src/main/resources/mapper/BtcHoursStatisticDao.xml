<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="hu.bme.bitcoin.dao.BtcHoursStatisticDao">


    <insert id="insertBtcHoursStatistic" useGeneratedKeys="true" keyProperty="id">
        insert into btc_hours_statistic(new_number, record_time) values(#{newNumber},#{recordTime})
    </insert>
</mapper>